CREATE TABLE files (
    file_id INT PRIMARY KEY,
    processid INT,
    description VARCHAR(255),
    name VARCHAR(255),
    file_type VARCHAR(255),
    FOREIGN KEY (processid) REFERENCES folder(processid)
);
